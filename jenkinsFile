pipeline {
    agent any
    
    stages{
        
    stage('Hello'){
        steps {
            echo 'hello world'
        }
    }
    
        stage('Build'){
        steps {
            echo 'Dev'
        }
    }
    
        stage('Test'){
        steps {
            echo 'Testing'
        }
    }
    
        stage('Deploy'){
        steps {
            echo 'Mise en production'
        }
    }
    }
}
